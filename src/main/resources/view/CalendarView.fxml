<VBox fx:id="vBoxCalendar" maxHeight="Infinity" maxWidth="Infinity" minHeight="-Infinity" minWidth="-Infinity"
      xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" VBox.vgrow="ALWAYS" HBox.hgrow="ALWAYS">

    <VBox BorderPane.alignment="CENTER" prefHeight="60" VBox.vgrow="NEVER" HBox.hgrow="ALWAYS">
        <children>
            <HBox prefHeight="26.0">
                <padding>
                    <Insets right="5" left="5" />
                </padding>
                <children>
                    <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="80.0" text="&lt;" />
                    <Region prefHeight="200.0" HBox.hgrow="ALWAYS" />
                    <ChoiceBox fx:id="monthSelection" prefWidth="150.0" />
                    <ChoiceBox fx:id="yearSelection" prefWidth="150.0" />
                    <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                    <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="80.0" text="&gt;" />
                </children>
            </HBox>
        </children>
    </VBox>
    <ScrollPane fx:id="scrollPane" VBox.vgrow="ALWAYS" HBox.hgrow="ALWAYS" prefHeight="300" minWidth="210.0">
        <GridPane gridLinesVisible="true" fx:id="gridPaneBottom" prefHeight="300" prefWidth="1000" BorderPane.alignment="CENTER" VBox.vgrow="ALWAYS" HBox.hgrow="ALWAYS">
            <columnConstraints>
                <ColumnConstraints hgrow="ALWAYS" minWidth="30.0" />
                <ColumnConstraints hgrow="ALWAYS" minWidth="30.0" />
                <ColumnConstraints hgrow="ALWAYS" minWidth="30.0" />
                <ColumnConstraints hgrow="ALWAYS" minWidth="30.0" />
                <ColumnConstraints hgrow="ALWAYS" minWidth="30.0" />
                <ColumnConstraints hgrow="ALWAYS" minWidth="30.0" />
                <ColumnConstraints hgrow="ALWAYS" minWidth="30.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="60.0" vgrow="NEVER" />
                <RowConstraints minHeight="60.0" vgrow="NEVER" />
                <RowConstraints minHeight="60.0" vgrow="ALWAYS" />
            </rowConstraints>
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="John Adam" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="John Beng" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="John Chew" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="John Tan" GridPane.columnIndex="3" GridPane.rowIndex="1"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="John Lim" GridPane.columnIndex="4" GridPane.rowIndex="1"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Chao Keng" GridPane.columnIndex="5" GridPane.rowIndex="1"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="John Travolta" GridPane.columnIndex="6" GridPane.rowIndex="1"/>
                <ListView fx:id="dutyPersonListView" VBox.vgrow="ALWAYS" GridPane.columnIndex="2" GridPane.rowIndex="2"/>
            </children>
        </GridPane>
    </ScrollPane>

</VBox>

